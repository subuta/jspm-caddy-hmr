{
  "name": "jspm-caddy-hmr",
  "description": "jspm & caddy => HMR :)",
  "version": "0.1.15",
  "registry": "npm",
  "dependencies": {
    "lodash": "^4.13.1",
    "commander": "^2.9.0",
    "sane": "^1.3.4"
  },
  "jspmPackage": true,
  "files": [
    "jspm-caddy-hmr.js",
    "package.json",
    "README.md",
    "dist",
    "lib"
  ],
  "browser": "dist/jspm-caddy-client.js",
  "main": "dist/jspm-caddy-client.js",
  "bin": {
    "jspm-caddy-hmr": "./jspm-caddy-hmr.js"
  },
  "format": "amd",
  "license": "MIT",
  "homepage": "https://github.com/subuta/jspm-caddy-hmr",
  "author": "subuta <shunta.3110@gmail.com> (https://github.com/subuta)",
  "bugs": {
    "url": "https://github.com/subuta/jspm-watch/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/subuta/jspm-watch.git"
  },
  "keywords": [
    "fs",
    "watch",
    "watcher",
    "jspm",
    "file",
    "caddy",
    "hmr",
    "module"
  ],
  "scripts": {
    "bundle": "jspm bundle example/app.js -wid",
    "unbundle": "jspm unbundle example/app.js",
    "serve": "caddy",
    "build-dev": "jspm build lib/jspm-caddy-client.js dist/jspm-caddy-client.js --format umd --global-name jspmCaddy",
    "build": "jspm build lib/jspm-caddy-client.js dist/jspm-caddy-client.js --format umd --global-name jspmCaddy --skip-source-maps --minify"
  },
  "jspm": {
    "configFile": "jspm.config.js",
    "dependencies": {
      "css": "github:systemjs/plugin-css@^0.1.22",
      "lodash-es": "^4.13.1"
    }
  }
}
