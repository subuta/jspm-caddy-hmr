{
  "name": "jspm-caddy-hmr",
  "description": "jspm & caddy => HMR :)",
  "version": "0.2.1",
  "registry": "npm",
  "dependencies": {
    "lodash": "^4.13.1",
    "commander": "^2.9.0",
    "sane": "^1.3.4"
  },
  "jspmPackage": true,
  "files": [
    "jspm-caddy-hmr.js",
    "package.json",
    "README.md",
    "dist",
    "lib"
  ],
  "browser": "dist/jspm-caddy-client.js",
  "main": "dist/jspm-caddy-hmr.js",
  "bin": {
    "jspm-caddy-hmr": "./jspm-caddy-hmr.js"
  },
  "format": "amd",
  "license": "MIT",
  "homepage": "https://github.com/subuta/jspm-caddy-hmr",
  "author": "subuta <shunta.3110@gmail.com> (https://github.com/subuta)",
  "bugs": {
    "url": "https://github.com/subuta/jspm-watch/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/subuta/jspm-watch.git"
  },
  "keywords": [
    "fs",
    "watch",
    "watcher",
    "jspm",
    "file",
    "caddy",
    "hmr",
    "module"
  ],
  "scripts": {
    "bundle": "jspm bundle example/app.js -wid",
    "unbundle": "jspm unbundle example/app.js",
    "serve": "caddy",
    "build-dev": "jspm build lib/jspm-caddy-client.js - lodash dist/jspm-caddy-client.js --format umd --global-name jspmCaddy --global-deps \"{'lodash':'lodash'}\"",
    "build": "jspm build lib/jspm-caddy-client.js - lodash dist/jspm-caddy-client.js --format umd --global-name jspmCaddy --global-deps \"{'lodash':'lodash'}\" --skip-source-maps --minify",
    "prepublish": "npm run build"
  },
  "jspm": {
    "configFile": "jspm.config.js",
    "dependencies": {
      "css": "github:systemjs/plugin-css@^0.1.23",
      "lodash": "^4.13.1",
      "path": "^0.12.7"
    },
    "devDependencies": {
      "events": "github:jspm/nodelibs-events@^0.2.0-alpha",
      "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
      "plugin-babel": "systemjs-plugin-babel@^0.0.12",
      "stream": "github:jspm/nodelibs-stream@^0.2.0-alpha"
    },
    "peerDependencies": {
      "assert": "github:jspm/nodelibs-assert@^0.2.0-alpha",
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "child_process": "github:jspm/nodelibs-child_process@^0.2.0-alpha",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha",
      "util": "github:jspm/nodelibs-util@^0.2.0-alpha",
      "vm": "github:jspm/nodelibs-vm@^0.2.0-alpha"
    },
    "overrides": {
      "npm:inherits@2.0.1": {
        "ignore": [
          "test.js"
        ]
      }
    }
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "electron-prebuilt": "^1.2.2",
    "jspm": "^0.17.0-beta.19",
    "sinon": "^1.17.4"
  }
}
